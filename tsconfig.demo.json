{
    "compilerOptions": {
		"target": "es2022",
		"module": "ES2022",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"strictPropertyInitialization": false,
		"outDir": "demo/CompiledEngine",
	},
	"tsc-alias": {
		"verbose": false,
		"resolveFullPaths": true,
	},
	"include": [
    	"src/**/*"
  	]
}
