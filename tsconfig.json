{
    "compilerOptions": {
		/* Visit https://aka.ms/tsconfig to read more about this file */
		"target": "es2022",
		"module": "ES2022",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"strictPropertyInitialization": false,
		"outDir": "dist",
		"declaration": true,
		"declarationMap": true,
	},
	"include": [
    	"src/**/*"
  	]
}
